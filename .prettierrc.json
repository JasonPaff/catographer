{
  "$schema": "https://json.schemastore.org/prettierrc",
  "cssDeclarationSorterOrder": "concentric-css",
  "endOfLine": "auto",
  "experimentalTernaries": false,
  "jsxSingleQuote": true,
  "plugins": [
    "prettier-plugin-css-order",
    "prettier-plugin-organize-imports"
  ],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 4,
  "tailwindFunctions": [
    "clsx",
    "cn",
    "cva",
    "cx"
  ],
  "trailingComma": "all"
}